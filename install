wget -q -nv -O idena-manager https://raw.githubusercontent.com/rioda-org/idena-manager/master/idena-manager
chmod +x idena-manager
wget -q -nv -O idena-manager-autocomplete.bash https://raw.githubusercontent.com/rioda-org/idena-manager/master/idena-manager-autocomplete.bash
./idena-manager install
export PATH=$(bash idena-manager set-path)
source $(idena-manager setup-autocomplete)
